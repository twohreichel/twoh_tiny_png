{
    "name": "twoh/twoh_tiny_png",
    "type": "typo3-cms-extension",
    "description": "Optimize your TYPO3 Images",
    "keywords": [
        "TYPO3",
        "extension",
        "image",
        "optimization",
        "tinypng"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Andreas Reichel",
            "role": "Developer",
            "email": "a.reichel91@outlook.com"
        },
        {
            "name": "Igor Smertin",
            "role": "Developer",
            "email": "igor.smertin@web.de"
        }
    ],
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "composer/installers": true
        }
    },
    "require": {
        "php": ">=8.0",
        "typo3/cms-core": "^12.4",
        "tinify/tinify": "^1.5.3",
        "ext-gd": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "TWOH\\TwohTinyPng\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "twoh_tiny_png"
        }
    }
}
