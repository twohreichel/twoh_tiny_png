{
    "name": "twoh/twoh_tiny_png",
    "type": "typo3-cms-extension",
    "description": "Optimize your TYPO3 Images using the TinyPNG API",
    "keywords": [
        "TYPO3",
        "extension",
        "image",
        "optimization",
        "tinypng",
        "bulk"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Andreas Reichel",
            "role": "Developer",
            "email": "a.reichel91@outlook.com"
        },
        {
            "name": "Igor Smertin",
            "role": "Developer",
            "email": "igor.smertin@web.de"
        }
    ],
    "version": "1.0.8",
    "support": {
        "issues": "https://github.com/twohreichel/twoh_tiny_png/issues",
        "source": "https://github.com/twohreichel/twoh_tiny_png",
        "docs": "https://docs.typo3.org/p/twoh/twoh_tiny_png/main/en-us/"
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "composer/installers": true
        }
    },
    "require": {
        "php": ">=8.2 <8.4",
        "typo3/cms-core": "^13.4",
        "tinify/tinify": "^1.6.3",
        "ext-gd": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^11.5",
        "friendsofphp/php-cs-fixer": "^3.91",
        "typo3/testing-framework": "^8.0"
    },
    "scripts": {
        "cs:fix": "php-cs-fixer fix",
        "cs:check": "php-cs-fixer fix --dry-run --diff",
        "test:unit": "phpunit -c Build/phpunit/UnitTests.xml"
    },
    "autoload": {
        "psr-4": {
            "TWOH\\TwohTinyPng\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TWOH\\TwohTinyPng\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "twoh_tiny_png"
        }
    }
}
